---
import '@styles/main.scss';

import { Footer, Navbar } from '@components';
import BaseLayout from './Base.astro';

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<BaseLayout title={title}>
  <slot name="preload" slot="preload" />
  <Navbar />
  <slot />
  <Footer />
</BaseLayout>
